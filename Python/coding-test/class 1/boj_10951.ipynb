    "# 10951\n",
    "import sys\n",
    "mlist = [sum(map(int, line.split())) for line in sys.stdin]\n",
    "print(*mlist, sep='\\n')\n",
    "\n",
    "# sys.stdin은 입력이 끝날 때까지 한 줄씩(line) 던져줍니다."
